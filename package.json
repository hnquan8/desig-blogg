{
  "name": "nextjs-notion-desig",
  "version": "2.0.0",
  "private": true,
  "description": "",
  "author": "Desig",
  "repository": "",
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "deploy": "next deploy",
    "deps": "run-s deps:*",
    "deps:update": "[ -z $GITHUB_ACTIONS ] && yarn add notion-client notion-types notion-utils react-notion-x || echo 'Skipping deps:update on CI'",
    "deps:link": "[ -z $GITHUB_ACTIONS ] && yarn link notion-client notion-types notion-utils react-notion-x || echo 'Skipping deps:link on CI'",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "test": "run-p test:*",
    "test:lint": "eslint '**/*.{ts,tsx}'",
    "test:prettier": "prettier '**/*.{js,jsx,ts,tsx}' --check"
  },
  "dependencies": {
    "@keyvhq/core": "^1.6.9",
    "@notionhq/client": "^2.2.8",
    "next": "^13.4.12",
    "notion-client": "^6.15.6",
    "notion-types": "^6.15.6",
    "notion-utils": "^6.15.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-notion-x": "^6.15.6",
    "react-tweet-embed": "^2.0.0",
    "react-use": "^17.3.2",
    "swiper": "9.4.1",
    "zustand": "^4.3.9"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.3.1",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "^18.8.5",
    "@types/react": "^18.0.21",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.2",
    "daisyui": "^3.3.1",
    "eslint": "^8.25.0",
    "eslint-config-next": "^13.4.12",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "got": "^13.0.0",
    "lqip-modern": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "p-map": "^6.0.0",
    "p-memoize": "^7.1.1",
    "postcss": "^8.4.27",
    "prettier": "^2.7.1",
    "sass": "^1.64.1",
    "tailwindcss": "^3.3.3",
    "typescript": "^4.8.4"
  },
  "overrides": {
    "cacheable-request": {
      "keyv": "npm:@keyvhq/core@~1.6.6"
    }
  }
